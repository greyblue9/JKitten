        <li><get var="size"/>:
                    <think><set var="items"><select><vars>?x</vars><q><subj>?x</subj><pred>hasSize</pred><obj><get var="size"/></obj></q></select></set><set var="size"><map><name>successor</name><get var="size"/></map></set></think><condition var="items"><li value="NIL"/><li><think><set var="head"><first><get var="items"/></first></set><set var="items"><rest><get var="items"/></rest></set></think>
                            "
                            <get var="?x"><tuple><get var="head"/></tuple></get>
                            "
