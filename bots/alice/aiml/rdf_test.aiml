SELECT TEST
<set var="tuples">
        <select>
          <vars>?x</vars>
          <q>
            <subj>?x</subj>
            <pred>hasSize</pred>
            <obj>7</obj>
          </q>
          <q>
            <subj>?x</subj>
            <pred>lifeArea</pred>
            <obj>Physical</obj>
          </q>
        </select>
      </set>
      <condition var="tuples">
        <li value="NIL"/>
        <li>
          <think>
            <set var="head">
              <first>
                <get var="tuples"/>
              </first>
            </set>
            <set var="tuples">
              <rest>
                <get var="tuples"/>
              </rest>
            </set>
          </think>
          <get var="?x">
            <tuple>
              <get var="head"/>
            </tuple>
          </get>
          <loop/>
        </li>
      </condition>

